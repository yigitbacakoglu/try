<div class="container-fluid">
  <div class="row-fluid">
    <div class="span12">
      <div class="box bordered">
        <div class="padded">
          <div class="section-title">Widgets</div>
          <table class="table table-striped">
            <thead>
            <tr>
              <th>Webpage</th>
              <th>Category</th>
              <th>Key</th>
            </tr>
            </thead>
            <tbody>
            <% @widgets.each do |widget| %>
                <tr>
                  <td>
                    <%= widget.webpage %>
                  </td>
                  <td style="line-height: 24px;">
                    <%= widget.category_id %>
                  </td>
                  <td>   <%= widget.key %>
                  </td>
                </tr>
            <% end %>

            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
